func! myFunc()
    print("hi")
end!

func! argFunc(arg1)
    // allocMemory is kinda dangerous
    // since this allocates memory
    // If you do not do freeMemory then it will cause memory leaks (Doesn't apply to all OSes)
    // Its okay if you don't use it since it will reclaim the memory used
    local test = allocMemory(sizeof(arg1))
    freeMemory(test)
end!

myFunc()