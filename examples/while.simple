func! hi()
    global i = 0
    while true == true do
        if i > 5 then
            break
        end
        i++
    end!
    return "asfj"
end!

func! fib(x)
    if x == 0 then
        return 0
    elseif 3 > x then
        return 1
    else
        global end_1 = fib->(x - 1)
        global end_2 = fib->(x - 2)
        return end_1 + end_2
    end
end!

global c = hi->()
print->(c)
global a = fib->(0)
print->(a)